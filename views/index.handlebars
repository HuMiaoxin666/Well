<main id="main" role="main" class="col-md-12 ml-sm-auto col-lg-12 " style="padding-left:0px;">
    <div id="left-view">
        <div id="infor_headline" class="head-bg-color" style=""><a id="infor_headline_a">Information View</a></div>
        <div id="infor" class="border-color">
            <span style="margin-left:10%;margin-top:5.5%;position:absolute;color:rgb(124, 124, 124);">井的总数： </span>
            <input type="text" id="well_count" value=1554>
            <span style="margin-left:10%;margin-top:20%;position:absolute;color:rgb(124, 124, 124);">标准井ID： </span>
            <input type="text" id="well_id" value=''>
            <span style="margin-left:10%;margin-top:35%;position:absolute;color:rgb(124, 124, 124);">当前盘内井数： </span>
            <input type="text" id="dish_count" value=''>
        </div>

        <div id="para_headline" class="head-bg-color" style=""><a id="para_headline_a">Control View</a></div>
        <div id="para_control" class="border-color">
            <span style="margin-left:10%;margin-top:5.5%;position:absolute;color:rgb(124, 124, 124);">采样率： </span>
            <input type="text" id="sample_rate" value=50>
            <div id="rate_slider"></div>
            <button id="sample" class="btn btn-outline-primary my-2 my-sm-0" type="submit">Sample</button>

            <div id="attrs_select" class="checkbox">
                <label class="checkbox-inline" id='check_1'>
                    <input type="checkbox" id="inlineCheckbox1" value="option1" > 重新计算系数
                </label>
                {{!-- <label class="radio-inline" id='check_1'>
                    <input type="radio" name="optionsRadiosinline" id="" value="option1" checked> 重新计算系数
                </label>
                <label class="radio-inline" id='check_2'>
                    <input type="radio" name="optionsRadiosinline" id="" value="option2"> 选项 2
                </label> --}}
            </div>
            <button id="match" class="btn btn-outline-primary my-2 my-sm-0" type="submit">Match</button>
        </div>

        <div id="other_headline" class="head-bg-color" style=""><a id="other_headline_a">Match View</a></div>
        <div id="other" class="border-color">
            <svg id="matchSvg"></svg>
        </div>
    </div>

    <div id="map_headline" class="head-bg-color">
        <a>Map View</a>
    </div>
    <div id="map" class="border-color"></div>

    <div id="rect_headline" class="head-bg-color">
        <a>Match Data View</a>
    </div>
    <div id="rectView" class="border-color">
        <svg id="SpSvg"></svg>
        <svg id="CondSvg"></svg>
        <svg id="Ml1Svg"></svg>
        <svg id="Ml2Svg"></svg>
        <svg id="AcSvg"></svg>
    </div>

    <div id="right-view">
        <div id="line_headline" class="head-bg-color">
            <a id="line_headline_a">Attribute Data View</a>
        </div>
        <div id="lineChart" class="border-color">
            <svg id="ChartSvg"></svg>
        </div>
        <div id="histogram_headline" class="head-bg-color">
            <a id="histogram_headline_a">Variance Data View</a>
        </div>
        <div id="histogram" class="border-color">
            <svg id="histogramSvg"></svg>
        </div>
